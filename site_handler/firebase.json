        "type": 302
      },
      {
        "source": "/aws.config.js",
        "destination": "/404.html",
        "type": 302
      },
      {
        "source": "/admin/**",
        "destination": "/404.html",
        "type": 302
      },
      {
        "source": "/.gitlab-ci.yml",
        "destination": "/404.html",
        "type": 302
      },
      {
        "source": "/config.json",
        "destination": "/404.html",
        "type": 302
      },
      {
        "source": "/config.js",
