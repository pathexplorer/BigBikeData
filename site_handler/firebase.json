      },
      {
        "source": "/aws-config.js",
        "destination": "/404.html",
        "type": 302
      },
      {
        "source": "/sftp-config.json",
        "destination": "/404.html",
        "type": 302
      },
      {
        "source": "/sftp.json",
        "destination": "/404.html",
        "type": 302
      },
      {
        "regex": "/xmlrpc\\.php\\?.*",
        "destination": "/404.html",
        "type": 302
      }
    ],
    "rewrites":
    [
      {
        "source": "/",
        "run": {
          "serviceId": "front-side-for-friends",
          "region": "us-central1"
        }
      },
      {
        "source": "/language/en",
        "run": {
          "serviceId": "front-side-for-friends",
          "region": "us-central1"
        }
      },
      {
        "source": "/language/uk",
        "run": {
          "serviceId": "front-side-for-friends",
          "region": "us-central1"
        }
      },
      {
        "source": "/upload",
        "run": {
          "serviceId": "front-side-for-friends",
          "region": "us-central1"
        }
      },
      {
        "source": "/success",
        "run": {
          "serviceId": "front-side-for-friends",
          "region": "us-central1"
        }
      },
      {
        "source": "/robots.txt",
        "run": {
          "serviceId": "front-side-for-friends",
          "region": "us-central1"
        }
      },
      {
        "source": "/download/**",
        "run": {
          "serviceId": "front-side-for-friends",
          "region": "us-central1"
        }
      },
      {
        "source": "**",
        "destination": "/static/404.html"
      }
    ],
    "headers": [
      {
        "source": "/static/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "**/*.@(html)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          }
        ]
      },
      {
        "source": "**",
        "headers": [
          {
            "key": "Strict-Transport-Security",
            "value": "max-age=31536000; includeSubDomains; preload"
          }
        ]
      }
    ]
  }
}
